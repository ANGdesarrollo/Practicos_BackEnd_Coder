{{#if products}}
    {{#each products}}
        <tr>
            <th scope="row">{{id}}</th>
            <th scope="row">{{title}}</th>
            <th scope="row">US ${{price}}</th>
            <th scope="row"><img class="img-list" src={{thumbnail}} alt={{title}}></th>
        </tr>
    {{/each}}
{{/if}}